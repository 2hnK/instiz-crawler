{
  "list_scope": [
    "#mboard list"
  ],
  "list_item_link": [
    "td.listsubject > a"
  ],
  "list_comment_count": [
    ".sbj span[class*='cmt']"
  ],
  "exclude_scopes": [
    "#ingreen",
    ".realchart_item",
    "#mboard td.no_mouseover",
    ".listsubject .list_subtitle",
    ".between_house",
    "#sense14"
  ],
  "comments": {
    "item": "table[node='_44'] tr",
    "content": "td",
    "time": "span.time"
  },
  "comment_pager": {
    "anchors": "td[node='_45'] a, td[node='_127'] a, table#indextable td[class^='indexing'] a[href^=\"javascript:ajax\"]",
    "js_pattern": "javascript:ajax\\s*c?\\s*\\(\\s*'(?P<board>[^']+)'\\s*,\\s*(?P<id>\\d+)\\s*,\\s*(?P<page>\\d+)",
    "url_template": "{base}/bbs/view_comment.php?id={board}&no={id}&cmt={cmt}&xp={page}&xb=1"
  },
  "post_title": [
    "#nowsubject"
  ],
  "post_body": [
    "#memo_content_1"
  ],
  "post_body_strict": [
    "#memo_content_1 p",
    "#memo_content_1 .text",
    "#memo_content_1"
  ],
  "post_comment_count": [
    "#nowsubject span"
  ],
  "post_images": [
    "#memo_content_1 img"
  ]
}
